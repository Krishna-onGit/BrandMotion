{
    "animations": [
        {
            "id": "fadeIn",
            "name": "Fade In",
            "description": "Smooth opacity fade from invisible to visible",
            "category": "basic",
            "duration": 1.5,
            "easing": "ease-out",
            "keyframes": {
                "0%": {
                    "opacity": 0
                },
                "100%": {
                    "opacity": 1
                }
            }
        },
        {
            "id": "slideUp",
            "name": "Slide Up",
            "description": "Text slides up from below with fade",
            "category": "motion",
            "duration": 1.2,
            "easing": "cubic-bezier(0.16, 1, 0.3, 1)",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "translateY(60px)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "translateY(0)"
                }
            }
        },
        {
            "id": "slideLeft",
            "name": "Slide Left",
            "description": "Text slides in from the right",
            "category": "motion",
            "duration": 1.2,
            "easing": "cubic-bezier(0.16, 1, 0.3, 1)",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "translateX(80px)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "translateX(0)"
                }
            }
        },
        {
            "id": "slideRight",
            "name": "Slide Right",
            "description": "Text slides in from the left",
            "category": "motion",
            "duration": 1.2,
            "easing": "cubic-bezier(0.16, 1, 0.3, 1)",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "translateX(-80px)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "translateX(0)"
                }
            }
        },
        {
            "id": "scaleIn",
            "name": "Scale In",
            "description": "Text scales up from small with elastic feel",
            "category": "motion",
            "duration": 1.0,
            "easing": "cubic-bezier(0.34, 1.56, 0.64, 1)",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "scale(0.5)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "scale(1)"
                }
            }
        },
        {
            "id": "typewriter",
            "name": "Typewriter",
            "description": "Character-by-character typing effect",
            "category": "special",
            "duration": 2.5,
            "easing": "steps(40, end)",
            "isTypewriter": true,
            "keyframes": {
                "0%": {
                    "width": "0"
                },
                "100%": {
                    "width": "100%"
                }
            },
            "cursorKeyframes": {
                "0%, 100%": {
                    "opacity": 1
                },
                "50%": {
                    "opacity": 0
                }
            }
        },
        {
            "id": "glowEntry",
            "name": "Glow Entry",
            "description": "Text fades in with glowing effect",
            "category": "special",
            "duration": 1.8,
            "easing": "ease-out",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "textShadow": "0 0 0px transparent",
                    "filter": "blur(8px)"
                },
                "50%": {
                    "opacity": 0.8,
                    "textShadow": "0 0 30px currentColor, 0 0 60px currentColor",
                    "filter": "blur(2px)"
                },
                "100%": {
                    "opacity": 1,
                    "textShadow": "0 0 10px currentColor",
                    "filter": "blur(0px)"
                }
            }
        },
        {
            "id": "bounceIn",
            "name": "Bounce In",
            "description": "Text bounces in with spring effect",
            "category": "motion",
            "duration": 1.4,
            "easing": "ease-out",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "scale(0.3) translateY(-100px)"
                },
                "50%": {
                    "opacity": 1,
                    "transform": "scale(1.05) translateY(0)"
                },
                "70%": {
                    "transform": "scale(0.95)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "scale(1)"
                }
            }
        },
        {
            "id": "zoomBlur",
            "name": "Zoom Blur",
            "description": "Cinematic zoom with motion blur",
            "category": "cinematic",
            "duration": 1.5,
            "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "scale(3)",
                    "filter": "blur(20px)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "scale(1)",
                    "filter": "blur(0)"
                }
            }
        },
        {
            "id": "flipIn",
            "name": "Flip In",
            "description": "3D flip rotation entry",
            "category": "3d",
            "duration": 1.2,
            "easing": "ease-out",
            "keyframes": {
                "0%": {
                    "opacity": 0,
                    "transform": "perspective(400px) rotateX(-90deg)"
                },
                "40%": {
                    "opacity": 1,
                    "transform": "perspective(400px) rotateX(20deg)"
                },
                "60%": {
                    "transform": "perspective(400px) rotateX(-10deg)"
                },
                "80%": {
                    "transform": "perspective(400px) rotateX(5deg)"
                },
                "100%": {
                    "opacity": 1,
                    "transform": "perspective(400px) rotateX(0)"
                }
            }
        }
    ],
